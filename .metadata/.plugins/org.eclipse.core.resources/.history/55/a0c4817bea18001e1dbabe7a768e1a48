package com.practise.fundamentals;

public class Amstrongnumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int input = 343;
        int temp = input, output = 0, r = 0;
        while(temp !=0) {
           r = temp%10;
            output = (r*r*r);
            temp = temp /10;
        }
        
        if(input == output) {
            System.out.println("input is a Amstrong number");
        }else {
            System.out.println("input is not a amstrong number");
        }
 
    }

	}


